<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="idAdd" class="cpc.pune.dipfive.Address">
  <constructor-arg value="Pune"></constructor-arg>
  <constructor-arg value="Maha"></constructor-arg>
  <constructor-arg value="Ind"></constructor-arg>
</bean>
  
<bean id="idStud" class="cpc.pune.dipfive.Student">
  <constructor-arg value="100" type="int"></constructor-arg>
  <constructor-arg value="Dhananjay"></constructor-arg>
  <constructor-arg value="22" type="int"></constructor-arg>
  <constructor-arg> <ref bean="idAdd"/> </constructor-arg>
</bean>  
  
</beans>  
