<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="idAdd" class="cpc.pune.dipfour.Address">
  <property name="city" value="Pune"></property>
  <property name="state" value="Maharashtra"></property>
  <property name="country" value="India"></property>
</bean>  
  
<bean id="idStud" class="cpc.pune.dipfour.Student">
  <property name="sno" value="100"></property>
  <property name="sname" value="Dhananjay"></property>
  <property name="age" value="22"></property>
  <property name="address" ref="idAdd"></property>
</bean>
  
</beans>  
