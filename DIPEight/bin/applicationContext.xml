<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
  <bean id="idStud" class="cpc.pune.dipeight.Student">
    <constructor-arg value="100" type="int"></constructor-arg>
    <constructor-arg value="Dhananjay"></constructor-arg>
    <constructor-arg value="22" type="int"></constructor-arg>
    
    <constructor-arg>
     <map>
       <entry key="Physic" value="96"></entry>
       <entry key="chemistry" value="97"></entry>
       <entry key="Maths" value="100"></entry>
     </map>
    </constructor-arg>
    
  </bean>

  
</beans>  
